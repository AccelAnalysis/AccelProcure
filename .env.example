# API Keys and Secrets
# Copy this file to .env and fill in your actual values before running the app

# ---------------------------------------------------------------------------
# Core App Settings (available in the browser via import.meta.env)
# ---------------------------------------------------------------------------
VITE_APP_NAME=AccelRFx
VITE_APP_ENV=development
VITE_API_BASE_URL=http://localhost:3000/api
VITE_PORT=5173

# ---------------------------------------------------------------------------
# Mapbox
# ---------------------------------------------------------------------------
VITE_MAPBOX_TOKEN=your_mapbox_access_token

# ---------------------------------------------------------------------------
# Supabase
# ---------------------------------------------------------------------------
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_public_anon_key

# Optional server-side keys (never expose these to the client)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# ---------------------------------------------------------------------------
# Firebase
# ---------------------------------------------------------------------------
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# ---------------------------------------------------------------------------
# OpenAI (server-side usage only)
# ---------------------------------------------------------------------------
OPENAI_API_KEY=your_openai_api_key
OPENAI_ORGANIZATION=optional_org_id

# ---------------------------------------------------------------------------
# Miscellaneous / Server Runtime
# ---------------------------------------------------------------------------
NODE_ENV=development
PORT=3000
# DATABASE_URL=your_database_connection_string
# SESSION_SECRET=your_session_secret
# STRIPE_SECRET_KEY=your_stripe_secret_key
